<html>
	<head>
		<title>Avaliação - Primeiro Trimestre</title>
		<meta charset="UTF-8">
		<link href="estilos.css" rel="stylesheet" type="text/css">
	</head>
	<body>
		<h1>Cadastro de Compras</h1>
		<hr>
        <a href="/formCompra">Nova Compra</a>
        <br><br>
		<table border="1" cellpadding="0" cellspacing="0">
            <tr>
                <th>Id</th>
                <th>Material</th>
                <th>Servidor</th>
                <th>Quantidade</th>
                <th>Urgência</th>
            </tr>
            <%
            compras.forEach((c, index) => {
            %>
            <tr>
                <td><%= c.id %></td>
                <td><%= c.nome_material %></td>
                <td><%= c.nome_servidor %></td>
                <td><%= c.quantidade %></td>
                <td><%= c.urgencia %></td>
            </tr>
            <%
            })
            %>
        </table>
	</body>
</html>